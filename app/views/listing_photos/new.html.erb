<%= form_for @listing_photo, :html => { :multipart => true} do |f| %>
    <div class="form-group">
      <%= f.label :caption %></br>
      <%= f.text_field :caption %>
    </div>
    <div class="form-group">
    <%= f.file_field :photo %>
    </div>
    <div class="form-group">
    <%= f.hidden_field :listing_id, :value => @listing.id %></br>
     <%= f.submit(:class => "btn btn-primary") %>
    </div>
    <% end %>
</div>
